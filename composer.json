{
    "name": "laravelso/site",
    "description": "http://www.laravel.so.",
    "keywords": ["Tricks", "framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.1.*",
        "barryvdh/laravel-debugbar": "^2.0@dev",
        "overtrue/wechat": "2.1.x-dev",
        "overtrue/laravel-lang": "1.0.*",
        "php": ">=5.5.9",
        "intervention/image": "2.3.*",
        "thomaswelton/laravel-gravatar": "1.0.*",
        "roumen/sitemap": "2.5.*",
        "laravel/socialite": "^3.0@dev",
        "illuminate/html": "^5.0@dev",
        "mccool/laravel-auto-presenter": "^4.0@dev",
        "overtrue/laravel-pinyin": "dev-master",
        "guzzlehttp/guzzle": "^6.0@dev",
        "hashids/hashids": "^1.0@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files" : ["app/Helpers.php"]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
